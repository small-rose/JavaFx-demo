<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="722.0" prefWidth="837.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cai.small.box.function.IpConfigController">
   <children>
       <StackPane fx:id="containerPane" prefHeight="594.0" prefWidth="868.0" AnchorPane.bottomAnchor="0.0"
                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
      <TabPane tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="查本机IP">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="722.0" prefWidth="837.0">
                     <children>
                        <GridPane layoutX="-5.0" layoutY="14.0" prefHeight="622.0" prefWidth="856.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="249.0" minWidth="10.0" prefWidth="78.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="585.0" minWidth="10.0" prefWidth="477.0" />
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="425.0" minWidth="10.0" prefWidth="273.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="57.0" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="495.0" minHeight="10.0" prefHeight="392.0" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="224.0" minHeight="10.0" prefHeight="187.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <TextArea fx:id="ipConfigTextArea" prefHeight="382.0" prefWidth="325.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                              <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1">
                                 <children>
                                    <Button fx:id="ipConfigButton" mnemonicParsing="false" onAction="#ipConfigOnAction" text="ipconfig">
                                       <HBox.margin>
                                          <Insets top="10.0" />
                                       </HBox.margin>
                                        <styleClass>
                                            <String fx:value="btn"/>
                                            <String fx:value="btn-info"/>
                                        </styleClass>
                                    </Button>
                                    <Button fx:id="ipConfigAllButton" onAction="#ipConfigAllOnAction" mnemonicParsing="false" text="ipconfig all">
                                       <HBox.margin>
                                          <Insets left="10.0" top="10.0" />
                                       </HBox.margin>
                                        <styleClass>
                                            <String fx:value="btn"/>
                                            <String fx:value="btn-success"/>
                                        </styleClass>
                                    </Button>
                                    <Button fx:id="ipConfigFlushButton" onAction="#ipConfigFlushOnAction" mnemonicParsing="false" text="ipconfig flush dns">
                                       <HBox.margin>
                                          <Insets left="10.0" top="10.0" />
                                       </HBox.margin>
                                        <styleClass>
                                            <String fx:value="btn"/>
                                            <String fx:value="btn-primary"/>
                                        </styleClass>
                                    </Button>
                                 </children>
                              </HBox>
                           </children>
                        </GridPane>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Ping地址">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                        <GridPane layoutY="22.0" prefHeight="701.0" prefWidth="500.0">
                          <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="335.0" minWidth="10.0" prefWidth="13.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="335.0" minWidth="10.0" prefWidth="82.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="640.0" minWidth="10.0" prefWidth="381.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints maxHeight="79.0" minHeight="10.0" prefHeight="34.0" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="89.0" minHeight="-Infinity" prefHeight="29.0" valignment="BASELINE" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="565.0" minHeight="10.0" prefHeight="386.0" vgrow="SOMETIMES" />
                              <RowConstraints maxHeight="481.0" minHeight="10.0" prefHeight="251.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Label text="IP或域名" GridPane.columnIndex="1" />
                              <TextField fx:id="ipValueText" GridPane.columnIndex="2" />
                               <Button fx:id="pingButton" mnemonicParsing="false" onAction="#pingButtonOnAction" text="Ping" GridPane.columnIndex="2" GridPane.rowIndex="1" >
                                   <styleClass>
                                       <String fx:value="btn"/>
                                       <String fx:value="btn-success"/>
                                   </styleClass>
                               </Button>
                              <TextArea fx:id="pingResultTextArea" prefHeight="386.0" prefWidth="362.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                           </children>
                        </GridPane>
                        <TitledPane layoutX="509.0" layoutY="17.0" prefHeight="456.0" prefWidth="174.0" text="历史记录">
                           <content>
                              <ListView fx:id="ipListHistory" prefHeight="229.0" prefWidth="200.0" />
                           </content>
                        </TitledPane>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
